{
  "name": "sync-calendar-module",
  "version": "0.1.6",
  "description": "Sync calendar module for Notion",
  "main": "build/cjs/index.js",
  "types": "build/cjs/types/*.d.ts",
  "scripts": {
    "clean": "rimraf ./build",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./tsconfig.esm.json && renamer -f .js -r .mjs ./build/esm/*",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "prepack": "npm run build",
    "test": "npx jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "renamer": "^4.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.3",
    "node-ical": "^0.15.3"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./build/esm/types/index.d.ts",
        "default": "./build/esm/index.mjs"
      },
      "require": {
        "types": "./build/cjs/types/index.d.ts",
        "default": "./build/cjs/index.js"
      }
    }
  },
  "files": [
    "build/**/*"
  ]
}
